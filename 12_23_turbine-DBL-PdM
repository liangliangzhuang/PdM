        "# 训练集-数据标记 - 生成 RUL（剩余使用寿命或故障时间）\n",
        "rul = pd.DataFrame(train_df.groupby('id')['cycle'].max()).reset_index()\n",
        "rul.columns = ['id', 'max']\n",
        "train_df = train_df.merge(rul, on=['id'], how='left')  # 合并到trai_df中\n",
        "train_df['RUL'] = train_df['max'] - train_df['cycle']\n",
        "train_df['RUL'] = [min(train_df['RUL'][i], 125) for i in range(np.shape(train_df['RUL'])[0])]  # 分段线性法\n",
        "train_df.drop('max', axis=1, inplace=True)"
