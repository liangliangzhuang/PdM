        "### 3.3.2 动态PdM中成本比率比较"
      ],
      "metadata": {
        "id": "p-caVAqoRYyO"
      }
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "MOEK7ds_P2gY"
      },
      "outputs": [],
      "source": [
        "# 所提方法 DPM\n",
        "# 这里对所有测试产品进行计算成本，选择max超过50的产品（97个）\n",
        "# 输出：\n",
        "T = 100\n",
        "product_id = 81\n",
        "real_cycle = [test_df[test_df['id'] == product_id].shape[0] for product_id in range(0,101)]\n",
        "# 预测\n",
        "pred_test_id = Pred_test_id(nlayer = nlayer, product_id = product_id, T = T, model = model, test_df = test_df,sequence_length = sequence_length,sequence_cols = sequence_cols)\n",
        "# 画图\n",
        "Plot_unit(case=case,pred_mean=pred_test_id[3], pred_std=pred_test_id[4], product_id=product_id, test_df=test_df,\n",
        "          sequence_length=sequence_length)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "j = 175 #其实是200\n",
        "m_re_all = [0,0,0,0]\n",
        "s0 = np.repeat(0,pred_test_id[1].shape[1])\n",
        "delta = np.repeat(1,pred_test_id[1].shape[1])\n",
        "K = 40\n",
        "future_time = pred_test_id[1].shape[1] + K #这里的40也要进行变化\n",
        "rul_time = pred_test_id[1][:,j] \n",
        "for i in range(j,future_time):\n",
        "  #j时刻，未来各点的DN和R成本比较 公式7，8\n",
        "  DT = i-j\n",
        "  CFP = np.mean([rul_time[h] <= DT for h in range(0,T)])\n",
        "  DN = np.sum([(cc + cos) * np.mean([r <= rul_time[h] < (r+1) for h in range(0,T)]) / (j+r) for r in range(0,DT+1)])\n",
        "  R = np.sum([(cc + cos) * np.mean([r <= rul_time[h] < (r+1) for h in range(0,T)]) / (j+r) for r in range(0,DT)]) + np.sum([np.mean([r <= rul_time[h] < (r+1) for h in range(0,T)]) * (cp + cf * (r - DT) * 1) / (j+DT) for r in range(DT+1,DT+K)])\n",
        "  m_re_all = np.vstack((m_re_all,[i,CFP,DN,R]))\n",
        "\n",
        "m_re_all = m_re_all[1:]"
      ],
      "metadata": {
        "id": "ZOpId-SSRn52"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
