        "def five_bar(size, case,scores, ymax):\n",
        "    plt.style.use('seaborn-white')\n",
        "    plt.figure(figsize=size, dpi=150)\n",
        "    names = ('Policy I', 'Policy II', 'Policy III', 'DPM', 'IPM')  # Name\n",
