        "def sensitive_analysis(scenario = 1,rang = range(81, 101)):\n",
        "  cc1 = [2,5,8,10,15,20]\n",
        "  each_engine_re1 = [0, 0, 0, 0, 0, 0]\n",
        "  each_engine_re2 = [0, 0, 0, 0, 0, 0]\n",
        "  each_engine_re3 = [0, 0, 0, 0, 0, 0]\n",
        "  real_cycle = [test_df[test_df['id'] == pro_id].shape[0] for pro_id in range(0, 101)]\n",
        "\n",
        "  for product_id in rang:  # 101\n",
        "      T1 = time.time()\n",
        "      pred_test_id = Pred_test_id(nlayer=nlayer, product_id=product_id, T=T, model=model, test_df=test_df,\n",
        "                                  sequence_length=sequence_length, sequence_cols=sequence_cols)\n",
        "      # 画图\n",
        "      #Plot_unit(case = case,pred_mean=pred_test_id[3], pred_std=pred_test_id[4], product_id=product_id, test_df=test_df,\n",
        "      #          sequence_length=sequence_length)\n",
        "\n",
        "      re1 = fff(pred_test_id=pred_test_id, const=case_choice(case=1),\n",
        "              T=T,cp=cp,cos=cos,cq=cq,cf=cf,cc=cc1[scenario]*cp,L=20,interval = 50,real_cycle = real_cycle)\n",
        "      re2 = fff(pred_test_id=pred_test_id, const=case_choice(case=2),\n",
        "              T=T,cp=cp,cos=cos,cq=cq,cf=cf,cc=cc1[scenario]*cp,L=20,interval = 40,real_cycle = real_cycle)\n",
        "      re3 = fff(pred_test_id=pred_test_id, const=case_choice(case=3),\n",
        "              T=T,cp=cp,cos=cos,cq=cq,cf=cf,cc=cc1[scenario]*cp,L=20,interval = 30,real_cycle = real_cycle)\n",
        "      # re[0][0].to_csv(\"Model_compare/paper_re/Table3_Case1_\" + \"Unit\" + str(product_id) +\"(1).csv\")\n",
        "      # pd.DataFrame(re[0][0]).to_excel(writer, sheet_name='re_' + str(product_id)) #存储数据\n",
        "      each_engine_re1 = np.vstack((each_engine_re1, re1[1]))\n",
        "      each_engine_re2 = np.vstack((each_engine_re2, re2[1]))\n",
        "      each_engine_re3 = np.vstack((each_engine_re3, re3[1]))\n",
        "      print(product_id)\n",
        "      #print(re1)\n",
        "      #print(re2)\n",
        "      #print(re3)\n",
        "      print(time.time() - T1)\n",
        "  each_engine_re1 = pd.DataFrame(each_engine_re1[1:], columns=['id', 'O*', 'M', 'M*', 'f', 'psi'])\n",
        "  each_engine_re2 = pd.DataFrame(each_engine_re2[1:], columns=['id', 'O*', 'M', 'M*', 'f', 'psi'])\n",
        "  each_engine_re3 = pd.DataFrame(each_engine_re3[1:], columns=['id', 'O*', 'M', 'M*', 'f', 'psi'])\n",
        "  # each_engine_re.to_csv(\"Model_compare/final/8.26_Case\" + str(case) + \"T\" + str(T) + \".csv\")\n",
        "  each_engine_re1[\"LR\"] = pd.DataFrame(each_engine_re1['M*']/each_engine_re1['f'])\n",
        "  each_engine_re2[\"LR\"] = pd.DataFrame(each_engine_re2['M*']/each_engine_re2['f'])\n",
        "  each_engine_re3[\"LR\"] = pd.DataFrame(each_engine_re3['M*']/each_engine_re3['f'])\n",
        "  \n",
        "  sensitive_re = pd.concat([each_engine_re1[['id',\"O*\",\"M\",\"M*\",\"LR\"]],\n",
        "           each_engine_re2[[\"O*\",\"M\",\"M*\",\"LR\"]],\n",
        "           each_engine_re3[[\"O*\",\"M\",\"M*\",\"LR\"]]],\n",
        "          axis=1)\n",
        "  each_engine_re1.to_excel(writer, sheet_name='each_re1') #存储数据 （测试集20个的结果）\n",
        "  each_engine_re2.to_excel(writer, sheet_name='each_re2') #存储数据\n",
        "  each_engine_re3.to_excel(writer, sheet_name='each_re3') #存储数据\n",
        "  sensitive_re.to_excel(writer, sheet_name='sensitive' + str(scenario)) #存储数据\n",
        "  return sensitive_re"
      ],
      "metadata": {
        "id": "FJbBQ3HfYiYV"
      },
