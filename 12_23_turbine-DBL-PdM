        "\n",
        "def cul_cost(dat,const,real_cycle,cp,cos,cq,cf,cc):\n",
        "  # 计算不同策略的成本率（三种）\n",
        "  \"\"\"# dpmI\"\"\"\n",
        "  five_reI = pd.DataFrame([five_para(id=i, data=dat, method=\"1\",L=L) for i in range(0, dat.shape[0])],\n",
        "                          columns=['id', 'delta', 'kappa', 'psi', 'X', 'final'])  # 0开始\n",
        "  #five_reI.to_excel(writer, sheet_name='five_re_I') #存储数据\n",
        "  dpmI = ([CR_r(id=i, para=five_reI,cp=cp,cos=cos,cq=cq,cf=cf,cc=cc) for i in range(0, dat.shape[0])])\n",
        "  # 20个测试产品成本率结果\n",
        "  cr_dpmI = np.sum(dpmI * five_reI['final']) / np.sum(five_reI['final'])\n",
        "  # 前10个测试产品成本率结果\n",
        "  cr_dpmI_first = np.sum(dpmI[0:10] * five_reI['final'][0:10]) / np.sum(five_reI['final'][0:10])\n",
        "  # 后10个测试产品成本率结果\n",
        "  cr_dpmI_second = np .sum(dpmI[10:20] * five_reI['final'][10:20]) / np.sum(five_reI['final'][10:20])\n",
        "  dpmI_count = five_reI['X']  # 1表示预防性维修\n",
        "  #print([cr_dpmI, cr_dpmI_first, cr_dpmI_second])\n",
        "\n",
        "  \"\"\"# 理想型\"\"\"\n",
        "  ipm_best = [Maint_best(const, t_best=real_cycle[i] - 25) + 25 for i in range(81, 101)]\n",
        "  ipm = cp / np.array(ipm_best)\n",
        "  cr_ipm = np.sum(ipm * ipm_best) / np.sum(ipm_best)\n",
        "  ipm_count = np.linspace(1, 1, len(ipm))  # 都是预防性维修\n",
        "  #cr_ipm\n",
        "\n",
        "  \"\"\"# 预防型\"\"\"\n",
        "  real_rul = np.array(real_cycle[81:])\n",
        "  mean_rul = np.floor(real_rul.mean())\n",
        "  cpm = np.arange(0.0, len(real_rul), 1)\n",
        "  cpm_count = np.arange(0.0, len(real_rul), 1)\n",
        "  cpm_best = [Maint_best(const, t_best=mean_rul - 25) + 25 for i in range(81, 101)]\n",
        "  for i in range(0, len(real_rul)):\n",
        "      if real_rul[i] >= mean_rul:\n",
        "          cpm[i] = (cp + cf * (real_rul[i] - mean_rul)) / mean_rul\n",
        "          cpm_count[i] = 1  # 预防性维修\n",
        "      else:\n",
        "          cpm[i] = (cos + cc) / real_rul[i]\n",
        "          cpm_count[i] = 0  # 纠正性维修\n",
        "\n",
        "  cr_cpm = cpm.sum() / len(real_rul)\n",
        "  #cr_cpm\n",
        "\n",
        "  \"\"\"# 绘图1\"\"\"\n",
        "  df1 = pd.DataFrame({'x_values': range(81, 101),\n",
        "                      'DPM': dpmI,\n",
        "                      'CPM': cpm,\n",
        "                      'IPM': ipm\n",
        "                      })\n",
        "  \"\"\"# 绘图2\"\"\"\n",
        "  re_data1 = [[np.sum(cpm_count == 0), np.sum(dpmI_count == 0), np.sum(ipm_count == 0)],\n",
        "                  [np.sum(cpm_count == 1), np.sum(dpmI_count == 1), np.sum(ipm_count == 1)]]\n",
        "\n",
        "  re_data2 = [\n",
        "      [cpm[cpm_count == 0].sum(), pd.Series(dpmI)[dpmI_count == 0].sum(),\n",
        "      ipm[ipm_count == 0].sum()],\n",
        "      [cpm[cpm_count == 1].sum(), pd.Series(dpmI)[dpmI_count == 1].sum(),\n",
        "      ipm[ipm_count == 1].sum()]]\n",
        "  re_aaa = pd.concat([pd.DataFrame(re_data1),pd.DataFrame(re_data2)])\n",
        "\n",
        "  \"\"\"# 绘图3\"\"\"\n",
        "  policy1 = [0.7, 0.72]  # RESS2019\n",
        "  policy2 = [0.54, 0.72]  #\n",
        "  policy3 = [0.52, 0.58]\n",
        "  # 这里有问题，代码需要修改\n",
        "  policy_ipm = np.round([ipm[0:10].mean(), ipm[11:].mean()], 2)\n",
        "  policy_pem = np.round([cpm[0:10].mean(), cpm[11:].mean()], 2)\n",
        "  dpm_I = np.round([cr_dpmI_first, cr_dpmI_second], 2)\n",
        "  # dpm_II = np.round([cr_dpmII_first,cr_dpmII_second],2)\n",
        "  scores = (policy1, policy2, policy3, dpm_I, policy_ipm)\n",
        "\n",
        "\n",
        "  \n",
        "  return [df1, re_data1, re_data2, scores]\n",
        "  #dpmI,cr_dpmI,cr_dpmI_first,cr_dpmI_second,dpmI_count,ipm,cr_ipm,ipm_count,cpm,cpm_count,cr_cpm"
      ],
      "metadata": {
        "id": "H_X2NW6dKNju"
